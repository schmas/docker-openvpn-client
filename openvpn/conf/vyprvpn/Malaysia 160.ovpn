client
dev tun
proto udp
remote my1.vyprvpn.com 1194
resolv-retry infinite
nobind
persist-key
persist-tun
persist-remote-ip
;ca /etc/openvpn/conf/vyprvpn/ca.crt
tls-remote my1.vyprvpn.com
auth-user-pass /config/openvpn-credentials.txt
comp-lzo
verb 3

script-security 2
up /etc/openvpn/conf/vyprvpn/up.sh
down /etc/openvpn/conf/vyprvpn/down.sh
down-pre
