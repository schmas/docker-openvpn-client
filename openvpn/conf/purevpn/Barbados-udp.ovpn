client
dev tun
proto udp
remote vleu-bb1-ovpn-udp.pointtoserver.com 53
persist-key
persist-tun
ca /etc/openvpn/conf/purevpn/ca.crt
tls-auth /etc/openvpn/conf/purevpn/Wdc.key 1
cipher AES-256-CBC
comp-lzo
verb 1
mute 20
route-method exe
route-delay 2
route 0.0.0.0 0.0.0.0
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
explicit-exit-notify 2
ifconfig-nowarn
auth-nocache 
