client
dev tun
proto udp
remote no.privateinternetaccess.com 1198
resolv-retry infinite
nobind
persist-key
persist-tun
cipher aes-128-cbc
auth sha1
tls-client
remote-cert-tls server
auth-user-pass /config/openvpn-credentials.txt
comp-lzo
verb 1
reneg-sec 0
crl-verify /etc/openvpn/conf/pia/crl.pem
ca /etc/openvpn/conf/pia/ca.crt
disable-occ
